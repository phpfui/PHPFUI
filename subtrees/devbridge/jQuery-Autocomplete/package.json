{
  "name": "devbridge-autocomplete",
  "version": "1.4.11",
  "description": "Autocomplete provides suggestions while you type into the text field.",
  "homepage": "https://github.com/devbridge/jQuery-Autocomplete",
  "author": "Tomas Kirda (https://twitter.com/tkirda)",
  "main": "dist/jquery.autocomplete.js",
  "types": "./typings/jquery.autocomplete.d.ts",
  "license": "MIT",
  "keywords": [
    "jquery-plugin"
  ],
  "scripts": {
    "build": "grunt build",
    "format": "prettier --write ./src/**",
    "lint": "eslint ./src"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/devbridge/jQuery-Autocomplete.git"
  },
  "peerDependencies": {
    "jquery": ">=1.9"
  },
  "devDependencies": {
    "@types/jquery": "^3.5.22",
    "eslint": "^8.51.0",
    "grunt": "^1.6.1",
    "grunt-contrib-uglify": "^5.2.2",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true,
    "trailingComma": "es5",
    "tabWidth": 4
  },
  "eslintConfig": {
    "env": {
      "browser": true
    },
    "extends": [
      "eslint:recommended"
    ],
    "globals": {
      "define": "readonly",
      "jQuery": "readonly",
      "module": "readonly",
      "require": "readonly"
    }
  },
  "files": [
    "dist/",
    "typings/jquery-autocomplete/*.d.ts",
    "readme.md"
  ]
}